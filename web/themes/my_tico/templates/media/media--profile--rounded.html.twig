{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}

<div class="rounded-full overflow-hidden w-6 h-6 mr-2">
  {% if content %}
    {{ content }}
  {% endif %}
</div>
